# Gemini AI API Key for strategy generation
GEMINI_API_KEY=your_gemini_api_key_here

# API URL for local development
VITE_API_URL=http://localhost:3001

# Helius RPC for Solana (Backend)
# Get your API key from: https://helius.dev
SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_helius_api_key_here

# Helius RPC for Solana (Frontend)
# Same as above - Vite requires VITE_ prefix for frontend env vars
VITE_SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_helius_api_key_here

# Optional: ShadowWire Mock Mode (set to false for production)
# SHADOWWIRE_MOCK=true

# Optional: Testing Mode
# TESTING_MODE=false

# Optional: CORS Origins (comma-separated for production)
# CORS_ORIGINS=https://your-frontend-domain.com
